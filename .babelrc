{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        [
          {
            "mode": "wrap",
            "ignoreFilenames": ["node_modules"]
          }
        ],
        [
          {
            "lodash": {
              "transform": "lodash/${member}",
              "preventFullImport": true
            }
          }
        ]
      ]
    }
  }
}
